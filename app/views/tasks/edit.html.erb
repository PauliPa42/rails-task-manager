<div class="container">
<h1>Update this Task:</h1>

  <%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= label_tag :completed, "Completed?" %>
  <%= f.check_box :completed %>
  <%= f.submit %>
  <% end %>
</div>
